{
    "name": "nofw/nofw",
    "description": "No framework at all!",
    "keywords": ["nofw", "framework", "fw", "skeleton"],
    "homepage": "https://nofw.io/",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Márk Sági-Kazár",
            "email": "mark.sagikazar@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1",
        "php-di/php-di": "^6.0",
        "doctrine/annotations": "^1.4",
        "nikic/fast-route": "^1.2",
        "zendframework/zend-diactoros": "^1.4",
        "nofw/foundation": "^0.2",
        "doctrine/cache": "^1.6",
        "filp/whoops": "^2.1",
        "twig/twig": "^2.3",
        "twig/extensions": "^1.4",
        "monolog/monolog": "^1.22",
        "nofw/emperror": "^0.1.0",
        "middlewares/fast-route": "^0.7.0",
        "middlewares/utils": "^0.11.0",
        "middlewares/whoops": "^0.4.0",
        "middlewares/php-session": "^0.3.0",
        "middlewares/request-handler": "^0.2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.1",
        "symfony/var-dumper": "^3.2",
        "vlucas/phpdotenv": "^2.4"
    },
    "suggest": {
        "nofw/session-handlers": "Advanced session handling, see https://nofw.io/docs/extras/advanced-session/"
    },
    "autoload": {
        "psr-4": {
            "Nofw\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Nofw\\Tests\\": "tests/"
        }
    }
}
