{
    "name": "nofw/nofw",
    "description": "No framework at all!",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Márk Sági-Kazár",
            "email": "mark.sagikazar@gmail.com"
        }
    ],
    "require": {
        "php-di/php-di": "^5.4",
        "doctrine/annotations": "^1.4",
        "nikic/fast-route": "^1.2",
        "middlewares/fast-route": "^0.6.0",
        "http-interop/http-factory-diactoros": "^0.3.0",
        "nofw/foundation": "dev-master",
        "doctrine/cache": "^1.6",
        "middlewares/utils": "^0.11.0",
        "filp/whoops": "^2.1",
        "middlewares/whoops": "^0.4.0",
        "skm/whoops-extra": "^0.1.0",
        "twig/twig": "^2.3",
        "monolog/monolog": "^1.22",
        "twig/extensions": "^1.4",
        "middlewares/php-session": "^0.3.0",
        "nofw/session-handlers": "^0.1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.1",
        "symfony/var-dumper": "^3.2"
    },
    "suggest": {
        "ext-apcu": "Recommended for caching in production"
    },
    "autoload": {
        "psr-4": {
            "Nofw\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Nofw\\Tests\\": "tests/"
        }
    }
}
